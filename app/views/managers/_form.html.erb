<%= form_for @manager do |f| %>
<%= render partial: 'bands/errors', locals: {model: @manager}%>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <%= f.label :email %>
    <%= f.text_field :email %>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit %>
<% end %>